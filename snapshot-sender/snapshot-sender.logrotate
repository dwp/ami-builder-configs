/var/log/snapshot-sender/snapshot-sender.out {
    missingok
    notifempty
    maxsize 1024k
    rotate 10
    yearly
    create 0640 snapshot-sender snapshot-sender
}

/var/log/snapshot-sender/snapshot-sender.err {
    missingok
    notifempty
    maxsize 1024k
    rotate 10
    yearly
    create 0640 snapshot-sender snapshot-sender
}
